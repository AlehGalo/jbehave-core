<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


  <bean id="traderSteps" class="org.jbehave.examples.trader.TraderSteps">
    <constructor-arg>
      <bean class="org.jbehave.examples.trader.service.TradingService"/>
    </constructor-arg>
  </bean>

  <bean class="org.jbehave.examples.trader.BeforeAfterSteps" />

  <bean class="org.jbehave.examples.trader.stories.AndStep$AndSteps" />

  <bean class="org.jbehave.examples.trader.stories.ClaimsWithNullCalendar$CalendarSteps" />

  <bean class="org.jbehave.examples.trader.stories.PriorityMatching$PriorityMatchingSteps" />

  <bean class="org.jbehave.examples.trader.stories.FailureFollowedByGivenStories$SandpitSteps" />

</beans>